class Solution {
    public int[] twoSum(int[] nums, int target) {
        int[] x = new int[2];
        int n = nums.length;
        int ind = 0;
        for(int i=0;i<n;i++){
            for(int j=i+1;j<n;j++){
        if (nums[i]+nums[j] == target) {x[ind]= i; ind++; x[ind]=j; break;}
            }
        } return x; 
    }
}